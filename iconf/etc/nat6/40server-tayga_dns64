# create new chain for nat64 packets
ip6tables -N nat64

# pass answers from nat64 to inet_in
ip6tables -A forward -s $NAT64Range -j inet_in
# pass packets to nat64 to nat64 chain
ip6tables -A forward -d $NAT64Range -j nat64
ip6tables -A nat64 -j inet_out

# allow nat64 packets from and to IServ
ip6tables -A forward -s $NAT64IPv6TunnelAddress -j ACCEPT
ip6tables -A forward -d $NAT64IPv6TunnelAddress -j ACCEPT
ip6tables -A forward -s $NAT64IPv6LocalAddress -j ACCEPT
ip6tables -A forward -d $NAT64IPv6LocalAddress -j ACCEPT

# do not restrict nat64 packets from IServ
ip6tables -t nat -I PREROUTING -s $NAT64IPv6TunnelAddress -j ACCEPT
ip6tables -t nat -I PREROUTING -s $NAT64IPv6LocalAddress -j ACCEPT

