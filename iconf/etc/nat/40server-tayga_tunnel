# Whitelist ipv4 alias range for nat64
iptables -A forward -s $NAT64IPv4Range -j ACCEPT
iptables -A forward -d $NAT64IPv4Range -m state --state RELATED,ESTABLISHED -j ACCEPT

# Allow traffic via nat64 tunnel
iptables -A forward -i nat64 -j ACCEPT
iptables -A forward -o nat64 -j ACCEPT

# No transparent squid for nat64
iptables -t nat -I forward -s $NAT64IPv4Range -j ACCEPT

